{
  "name": "electron-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "author": "authhub",
  "description": "SSP-PWLESS-AdminConsole",
  "build": {
    "asar": true,
    "appId": "com.react.electron",
    "productName": "react-electron",
    "files": [
      "build/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.24.5",
    "axios": "^1.8.4",
    "electron-is-dev": "^3.0.1",
    "moment-timezone": "^0.5.48",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.4.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dbuild": "npm run build && npm run electron-build",
    "electron-build": "electron-builder --publish never",
    "start-electron": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.1",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.3"
  }
}
